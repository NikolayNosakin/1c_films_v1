	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	СправочникКино.Ссылка,
	СправочникКино.ПометкаУдаления,
	СправочникКино.Код,
	СправочникКино.Наименование,
	СправочникКино.ГодВыпуска,
	СправочникКино.ГодОкончания,
	СправочникКино.КодIMDB,
	СправочникКино.КодКинопоиск,
	СправочникКино.КоличествоСезонов,
	СправочникКино.Описание,
	СправочникКино.ОригинальноеНазвание,
	СправочникКино.Тип,
	СправочникКино.Просмотрен,
	СправочникКино.Жанры,
	СправочникКино.Серии,
	СправочникКино.Страны,
	СправочникКино.Предопределенный,
	СправочникКино.ИмяПредопределенныхДанных
ИЗ
	Справочник.Кино КАК СправочникКино 
ГДЕ
	ВЫБОР
			КОГДА &amp;ФильтрПросмотр = 0
				ТОГДА ИСТИНА
			ИНАЧЕ ВЫБОР
					КОГДА &amp;ФильтрПросмотр = 1
						ТОГДА СправочникКино.Просмотрен = ИСТИНА
					ИНАЧЕ СправочникКино.Просмотрен = ЛОЖЬ
				КОНЕЦ
		КОНЕЦ
	И ВЫБОР
			КОГДА &amp;ТИП = ЗНАЧЕНИЕ(Перечисление.ТипыКино.ПустаяСсылка)
				ТОГДА ИСТИНА
			ИНАЧЕ СправочникКино.Тип = &amp;ТИП
		КОНЕЦ	</QueryText>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>Серии.НомерСтроки</dcssch:dataPath>
					<dcssch:field>Серии.НомерСтроки</dcssch:field>
				</Field>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>Ссылка</dcssch:dataPath>
