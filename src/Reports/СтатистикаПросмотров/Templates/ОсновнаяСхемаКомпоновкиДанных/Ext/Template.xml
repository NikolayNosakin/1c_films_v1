		<field xsi:type="DataSetFieldField">
			<dataPath>ПросмотреноКино</dataPath>
			<field>ПросмотреноКино</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Просмотрено кино</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Viewed movies</v8:content>
				</v8:item>
